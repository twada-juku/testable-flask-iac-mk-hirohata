{% extends 'base.html.jinja' %}

{% block title %}プロジェクト一覧{% endblock %}

{% block section_title %}プロジェクト一覧{% endblock %}

{% block content %}
  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead>
        <tr>
          <th scope="col">プロジェクト名</th>
        </tr>
      </thead>
      <tbody>
        {% for pj in projects %}
          <tr>
            <td><a href="{{ url_for('projects.show', url_name=pj.url_name) }}">{{ pj.name }}</a></td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
{% endblock %}
